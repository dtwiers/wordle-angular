    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/guesses/src/**/*.ts",
          "libs/guesses/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
